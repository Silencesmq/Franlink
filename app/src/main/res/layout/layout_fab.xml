<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.example.silence.franlink.jsong.FloatingDraftButton
        android:id="@+id/floatingActionButton"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:rippleColor="@color/colorPrimary"
        app:backgroundTint="#60363636"
        android:src="@drawable/boss"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        app:elevation="1dp"
        app:borderWidth="0dp"
        app:pressedTranslationZ="12dp"
        app:fabSize="mini"
        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_1"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:rippleColor="@color/colorPrimary"
        app:backgroundTint="#60363636"
        android:src="@drawable/sun1"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        app:elevation="1dp"
        app:borderWidth="0dp"
        app:pressedTranslationZ="12dp"
        app:fabSize="mini"
        android:visibility="invisible"
        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_2"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:rippleColor="@color/colorPrimary"
        app:backgroundTint="#60363636"
        android:src="@drawable/home"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        app:elevation="1dp"
        app:borderWidth="0dp"
        app:pressedTranslationZ="12dp"
        app:fabSize="mini"
        android:visibility="invisible"
        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_3"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:rippleColor="@color/colorPrimary"
        app:backgroundTint="#60363636"
        android:src="@drawable/line"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        app:elevation="1dp"
        app:borderWidth="0dp"
        app:pressedTranslationZ="12dp"
        app:fabSize="mini"
        android:visibility="invisible"
        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_4"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:rippleColor="@color/colorPrimary"
        app:backgroundTint="#60363636"
        android:src="@drawable/moon"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        app:elevation="1dp"
        app:borderWidth="0dp"
        app:pressedTranslationZ="12dp"
        app:fabSize="mini"
        android:visibility="invisible"
        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_5"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:rippleColor="@color/colorPrimary"
        app:backgroundTint="#60363636"
        android:src="@drawable/door"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        app:elevation="1dp"
        app:borderWidth="0dp"
        app:pressedTranslationZ="12dp"
        app:fabSize="mini"
        android:visibility="invisible"
        />
</RelativeLayout>